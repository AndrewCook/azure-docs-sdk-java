{
  "build": {
    "content": [
      {
        "files": [
          "**/*.yml"
        ],
        "exclude": [
          "**/toc.yml"
        ],
        "src": "api-ref",
        "dest": "."
      },
      {
        "files": [
          "toc.yml"
        ],
        "src": "api-ref",
        "dest": "."
      },
      {
        "files": [
          "index.md"
        ],
        "src": "api-index",
        "dest": "."
      },
      {
        "files": [
          "toc.yml"
        ],
        "src": "breadcrumb",
        "dest": "breadcrumb"
      },
      {
        "files": [
          "**/*.md"
        ],
        "src": "documentation",
        "dest": "documentation"
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "azure/sdk/java/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": "api-doc/*.md",
    "externalReference": [],
    "globalMetadata": {
      "author": "openpublishbuild",
      "ms.author": "douge",
      "manager": "douge",
      "ms.topic": "managed-reference",
      "ms.devlang": "java",
      "ms.date": "11/16/2016",
      "breadcrumb_path": "~/breadcrumb/toc.yml",
      "brand": "azure"
    },
    "fileMetadata": {
      "ms.service":
      {
        "api-ref/com.microsoft.azure.batch.**.yml": "batch"
      }
    },
    "template": [
      "docs.html"
    ],
    "dest": "azure/sdk/java"
  }
}
