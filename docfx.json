{
  "build": {
    "content": [
      {
        "files": [
          "**/*.yml"
        ],
        "exclude": [
          "**/toc.yml"
        ],
        "src": "api-ref",
        "dest": "."
      },
      {
        "files": [
          "toc.yml"
        ],
        "src": "api-ref",
        "dest": "."
      },
      {
        "files": [
          "index.md"
        ],
        "src": "api-index",
        "dest": "."
      },
      {
        "files": [
          "toc.yml"
        ],
        "src": "breadcrumb",
        "dest": "breadcrumb"
      },
      {
        "files": [
          "**/*.md"
        ],
        "src": "documentation",
        "dest": "documentation"
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "azure/sdk/java/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": "api-doc/*.md",
    "externalReference": [],
    "globalMetadata": {
      "author": "erickson-doug",
      "ms.author": "douge",
      "manager": "douge",
      "ms.topic": "reference",
      "ms.devlang": "java",
      "ms.date": "11/16/2016",
      "breadcrumb_path": "~/breadcrumb/toc.yml",
      "brand": "azure"
    },
    "fileMetadata": {
      "ms.service":
      {
        "api-ref/com.microsoft.azure.batch.**.yml": "batch",
        "api-ref/com.microsoft.azure.eventhubs.**.yml": "event-hubs",
        "api-ref/com.microsoft.azure.iot.**.yml": "iot-suite",
        "api-ref/com.microsoft.azure.iothub.**.yml": "iot-hubs",
        "api-ref/com.microsoft.azure.keyvault.**.yml": "key-vault",
        "api-ref/com.microsoft.azure.management.batch.**.yml": "batch",
        "api-ref/com.microsoft.azure.management.compute.**.yml": "big-compute",
        "api-ref/com.microsoft.azure.management.compute._virtual_machine_scale_set.**.yml": "virtual-machine-scale-sets",
        "api-ref/com.microsoft.azure.management.datalake.analytics.**.yml": "data-lake-analytics",
        "api-ref/com.microsoft.azure.management.datalake.store.**.yml": "data-lake-store",
        "api-ref/com.microsoft.azure.management.dns.**.yml": "dns",
        "api-ref/com.microsoft.azure.management.graphrbac**.yml": "graphrbac",
        "api-ref/com.microsoft.azure.management.keyvault.**.yml": "key-vault",
        "api-ref/com.microsoft.azure.management.network.**.yml": "virtual-network",
        "api-ref/com.microsoft.azure.management.redis.**.yml": "redis-cache",
        "api-ref/com.microsoft.azure.management.resources.**.yml": "azure-resource-manager",
        "api-ref/com.microsoft.azure.management.sql.**.yml": "sql-database",
        "api-ref/com.microsoft.azure.management.storage.**.yml": "storage",
        "api-ref/com.microsoft.azure.management.trafficmanager.**.yml": "traffic-manager",
        "api-ref/com.microsoft.azure.management.website.**.yml": "websites",
        "api-ref/com.microsoft.azure.servicebus.**.yml": "service-bus",
        "api-ref/com.microsoft.azure.storage.**.yml": "storage"
      }
    },
    "template": [
      "docs.html"
    ],
    "dest": "azure/sdk/java"
  }
}
